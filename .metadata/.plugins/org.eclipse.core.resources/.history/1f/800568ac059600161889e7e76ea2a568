package Chap1;

public class Ex3 {
	public static String removeDup(char [] arr){
		for(int j = 0; j < arr.length; j++){
			int k;
			for(k= 0; k < i; k++){
				if(arr[k] == arr[j])
					break;
			}
			if(k==i){
				arr[i++] = arr[j];
			}
		}
		
		return new String(arr,0,i);
	}
	
	public static void main(String []args){
		
	}
}
