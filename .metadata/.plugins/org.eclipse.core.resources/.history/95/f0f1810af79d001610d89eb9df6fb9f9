package Chap7;

public class Carta {
	public static enum Naipe{
		Espadas,
		Copas,
		Paus,
		Ouros
	}
	
	public static enum Tipo{
		Dois,
		Tres,
		Quatro,
		Cinco,
		Seis,
		Sete,
		Oito,
		Nove,
		Dama,
		Valete,
		Rei;
		
		public int valor(){
			
		}
	}
	
	private Naipe naipe;
	private Tipo tipo;
	
	public Carta(Naipe naipe, Tipo tipo){
		this.naipe = naipe;
		this.tipo = tipo;
	}
}
