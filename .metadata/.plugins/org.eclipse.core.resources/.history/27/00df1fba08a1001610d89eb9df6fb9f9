package Chap7Ex7;

import java.util.GregorianCalendar;

public class Message {
	private User source;
	private String text;
	private GregorianCalendar time;
	
	
	public Message(User source, String text, GregorianCalendar time){
		this.source = source;
		this.text = text;
		this.time = time;
	}
	
	public String getText(){
		return this.text;
	}
	
	public User getSource(){
		return this.source;
	}
	
	
}
