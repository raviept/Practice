package Chap3;

import DataStructures.Stack;
import Exceptions.EmptyStackException;

public class Ex6 {
	public static <T extends Comparable<T>> void sort(Stack<T> st){
		if(st.isEmpty())
			return;
		
		try{
			boolean change;

			Stack<T> aux = new Stack<>();
			do{
				change = false;
				T tVal = st.pop();

			}while(change);
		}
		catch(EmptyStackException e){
			e.printStackTrace();
		}
	}
}
