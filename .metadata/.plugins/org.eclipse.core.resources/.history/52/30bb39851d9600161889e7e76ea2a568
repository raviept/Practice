package Gen;

import java.util.Random;

public class RandGen {
	public static int [][] genMat(int m, int n, int max){
		int [][] mat = new int[m][n];
		Random rand = new Random();
		
		for(int i = 0; i < m; i++){
			for(int j = 0; j < n; j++){
				mat[i][j] = rand.nextInt(max);
			}
		}		
		
		return mat;
	}
	
	public static LinkNode<Integer> genLink(int n, int max){
		if(n <= 0)
			return null;
		Random rand = new Random();
		
		
	}
}
